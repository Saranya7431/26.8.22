#include<stdio.h>
#include<string.h>
int main()
{
	char str[10];
	printf("Enter string\n");
	gets(str);	
	int n;
	n=strlen(str);
	int i=0,j=n-1;
	while(i<j/2)
	{
		if(str[i]==str[j])
		{
			printf("Palindrome\n");
			i++;
			j--;
		}
		else
		{
			printf("Not Palindrome\n");
			i++;
			j--;
		}
        }
	return 0;
}
